    "plugins": [
      "transform-es2015-modules-commonjs",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "env": {
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    }
  },
  "scripts": {
    "create": "webpack",
    "start": "webpack-dev-server --open",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watch --collect-coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
